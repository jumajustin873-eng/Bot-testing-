<xml xmlns="https://developers.google.com/blockly/xml" is_dbot="true" collection="false">
  <variables>
    <variable id="aD0I2PPK3p_}A@z(vb0P">STAKE</variable>
    <variable id="=RZ25D}(}1+x^lQYjm6x">INITIAL STAKE</variable>
    <variable id="fCxrNtvK86,pspiRZF?H">STOP LOSS</variable>
    <variable id="NDjP(.1O#Q;|,|Ri0qmI">TAKE PROFIT</variable>
    <variable id="F9Hixb-C#0?IY2W5a`)g">MARTIGALE</variable>
    <variable id="2[7kM5JUtnNSKd/DZyNU">prediction</variable>
    <variable id="DIGIT_1">DIGIT_1</variable>
    <variable id="DIGIT_2">DIGIT_2</variable>
    <variable id="DIGIT_3">DIGIT_3</variable>
    <variable id="DIGIT_4">DIGIT_4</variable>
    <variable id="DIGIT_5">DIGIT_5</variable>
