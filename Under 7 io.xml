  </variables>

  <!-- Trade Definition -->
  <block type="trade_definition" id="vX#h+;e~!d-ecvc_9ix*" deletable="false" x="0" y="0">
    <statement name="TRADE_OPTIONS">
      <block type="trade_definition_market" id=")8@Q0Oz74@Id^2]pdp}+" deletable="false" movable="false">
        <field name="MARKET_LIST">synthetic_index</field>
        <field name="SUBMARKET_LIST">random_index</field>
        <field name="SYMBOL_LIST">1HZ10V</field>
        <next>
          <block type="trade_definition_tradetype" id="dT$[CT/fEB-hte6iXKlP" deletable="false" movable="false">
            <field name="TRADETYPECAT_LIST">digits</field>
            <field name="TRADETYPE_LIST">overunder</field>
            <next>
              <block type="trade_definition_contracttype" id="Ou#-i){=J,^|zX,.E0#[" deletable="false" movable="false">
                <field name="TYPE_LIST">DIGITUNDER</field>
                <next>
                  <block type="trade_definition_candleinterval" id="LP7?Irva%eEp7s+_/~g8" deletable="false" movable="false">
                    <field name="CANDLEINTERVAL_LIST">60</field>
                    <next>
                      <block type="trade_definition_restartbuysell" id="_@Iv=uV,:FO})`~w#dk=" deletable="false" movable="false">
                        <field name="TIME_MACHINE_ENABLED">FALSE</field>
                        <next>
                          <block type="trade_definition_restartonerror" id="2O4Y`Rv^0l^g4cfk;G-U" deletable="false" movable="false">
                            <field name="RESTARTONERROR">TRUE</field>
                          </block>
                        </next>
                      </block>
                    </next>
                  </block>
                </next>
              </block>
            </next>
          </block>
        </next>
      </block>
    </statement>

    <!-- Initialization -->
    <statement name="INITIALIZATION">
      <block type="variables_set" id="Dp4S!)WpVdwa%~V_4yV!">
        <field name="VAR">STAKE</field>
        <value name="VALUE">
          <block type="math_number"><field name="NUM">4.97</field></block>
        </value>
        <next>
          <block type="variables_set" id="IgiD^^KLv$Y6F._0hV#K">
            <field name="VAR">INITIAL STAKE</field>
            <value name="VALUE"><block type="math_number"><field name="NUM">4.97</field></block></value>
            <next>
              <block type="variables_set" id="Of/.G!Rq*4Wd.D|@xN-a">
                <field name="VAR">STOP LOSS</field>
                <value name="VALUE"><block type="math_number"><field name="NUM">100</field></block></value>
                <next>
                  <block type="variables_set" id="X#XYb8Q;pK@iYAp3w~pV">
                    <field name="VAR">TAKE PROFIT</field>
                    <value name="VALUE"><block type="math_number"><field name="NUM">50</field></block></value>
                    <next>
                      <block type="variables_set" id="y/AY@x+oVNn{ml,)0EFZ">
                        <field name="VAR">MARTIGALE</field>
                        <value name="VALUE"><block type="math_number"><field name="NUM">2</field></block></value>
                        <next>
                          <block type="variables_set" id="_RSvtmvL+tN|CFEG;4Bm">
                            <field name="VAR">prediction</field>
                            <value name="VALUE"><block type="math_number"><field name="NUM">7</field></block></value>
                          </block>
                        </next>
                      </block>
                    </next>
                  </block>
                </next>
              </block>
            </next>
          </block>
        </next>
      </block>
    </statement>

    <!-- Main Loop -->
    <statement name="SUBMARKET">
      <block type="controls_repeat_ext" id="a@N!8d@$6oUgWxkIc;u^">
        <value name="TIMES">
          <block type="math_constant"><field name="CONSTANT">INFINITY</field></block>
        </value>
        <statement name="DO">
          <block type="timeout">
            <statement name="TIMEOUTSTACK">

              <!-- Get last 5 digits -->
              <block type="variables_set" id="get_digits">
                <field name="VAR">DIGIT_1</field>
                <value>
                  <block type="lists_getIndex">
                    <field name="WHERE">FROM_END</field>
                    <field name="MODE">GET</field>
                    <value name="VALUE"><block type="lastDigitList"/></value>
                    <value name="AT"><block type="math_number"><field name="NUM">1</field></block></value>
                  </block>
                </value>
                <next>
                  <block type="variables_set">
                    <field name="VAR">DIGIT_2</field>
                    <value>
                      <block type="lists_getIndex">
                        <field name="WHERE">FROM_END</field>
                        <field name="MODE">GET</field>
                        <value name="VALUE"><block type="lastDigitList"/></value>
                        <value name="AT"><block type="math_number"><field name="NUM">2</field></block></value>
                      </block>
                    </value>
                    <next>
                      <block type="variables_set">
                        <field name="VAR">DIGIT_3</field>
                        <value>
                          <block type="lists_getIndex">
                            <field name="WHERE">FROM_END</field>
                            <field name="MODE">GET</field>
                            <value name="VALUE"><block type="lastDigitList"/></value>
                            <value name="AT"><block type="math_number"><field name="NUM">3</field></block></value>
                          </block>
                        </value>
                        <next>
                          <block type="variables_set">
                            <field name="VAR">DIGIT_4</field>
                            <value>
                              <block type="lists_getIndex">
                                <field name="WHERE">FROM_END</field>
                                <field name="MODE">GET</field>
                                <value name="VALUE"><block type="lastDigitList"/></value>
                                <value name="AT"><block type="math_number"><field name="NUM">4</field></block></value>
                              </block>
                            </value>
                            <next>
                              <block type="variables_set">
                                <field name="VAR">DIGIT_5</field>
                                <value>
                                  <block type="lists_getIndex">
                                    <field name="WHERE">FROM_END</field>
                                    <field name="MODE">GET</field>
                                    <value name="VALUE"><block type="lastDigitList"/></value>
                                    <value name="AT"><block type="math_number"><field name="NUM">5</field></block></value>
                                  </block>
                                </value>
                              </block>
                            </next>
                          </block>
                        </next>
                      </block>
                    </next>
                  </block>
                </next>
              </block>

              <!-- Check last 5 digits >= 7 -->
              <block type="controls_if">
                <value name="IF0">
                  <block type="logic_operation">
                    <field name="OP">AND</field>
                    <value name="A">
                      <block type="logic_operation">
                        <field name="OP">AND</field>
                        <value name="A">
                          <block type="logic_operation">
                            <field name="OP">AND</field>
                            <value name="A">
                              <block type="logic_compare">
                                <field name="OP">GTE</field>
                                <value name="A"><variables_get><field name="VAR">DIGIT_1</field></variables_get></value>
                                <value name="B"><math_number><field name="NUM">7</field></math_number></value>
                              </block>
                            </value>
                            <value name="B">
                              <block type="logic_compare">
                                <field name="OP">GTE</field>
                                <value name="A"><variables_get><field name="VAR">DIGIT_2</field></variables_get></value>
                                <value name="B"><math_number><field name="NUM">7</field></math_number></value>
                              </block>
                            </value>
                          </block>
                        </value>
                        <value name="B">
                          <block type="logic_compare">
                            <field name="OP">GTE</field>
                            <value name="A"><variables_get><field name="VAR">DIGIT_3</field></variables_get></value>
                            <value name="B"><math_number><field name="NUM">7</field></math_number></value>
                          </block>
                        </value>
                      </block>
                    </value>
                    <value name="B">
                      <block type="logic_operation">
                        <field name="OP">AND</field>
                        <value name="A">
                          <block type="logic_compare">
                            <field name="OP">GTE</field>
                            <value name="A"><variables_get><field name="VAR">DIGIT_4</field></variables_get></value>
                            <value name="B"><math_number><field name="NUM">7</field></math_number></value>
                          </block>
                        </value>
                        <value name="B">
                          <block type="logic_compare">
                            <field name="OP">GTE</field>
                            <value name="A"><variables_get><field name="VAR">DIGIT_5</field></variables_get></value>
                            <value name="B"><math_number><field name="NUM">7</field></math_number></value>
                          </block>
                        </value>
                      </block>
                    </value>
                  </block>
                </value>
                <statement name="DO0">
                  <block type="variables_set">
                    <field name="VAR">prediction</field>
                    <value>
                      <block type="math_number"><field name="NUM">7</field></block>
                    </value>
                    <next>
                      <block type="before_purchase">
                        <statement name="BEFOREPURCHASE_STACK">
                          <block type="purchase">
                            <field name="PURCHASE_LIST">DIGITUNDER</field>
                          </block>
                        </statement>
                      </block>
                    </next>
                  </block>
                </statement>
              </block>

            </statement>
            <value name="SECONDS"><block type="math_number"><field name="NUM">0</field></block></value>
          </block>
        </statement>
      </block>
    </statement>
  </block>

  <!-- After purchase logic -->
  <block type="after_purchase" id="_ka1u1SwqD,}H%!SN;fE" x="0" y="1800">
    <statement name="AFTERPURCHASE_STACK">
      <block type="controls_if">
        <mutation xmlns="http://www.w3.org/1999/xhtml" elseif="1" else="1"></mutation>
        <value name="IF0">
          <block type="logic_compare">
            <field name="OP">GTE</field>
            <value name="A"><block type="total_profit"/></value>
            <value name="B"><variables_get><field name="VAR">TAKE PROFIT</field></variables_get></value>
          </block>
        </value>
        <statement name="DO0">
          <block type="text_print">
            <value name="TEXT"><shadow type="text"><field name="TEXT">THATS IT BUDDY</field></shadow></value>
          </block>
        </statement>
        <value name="IF1">
          <block type="logic_compare">
            <field name="OP">LTE</field>
            <value name="A"><block type="total_profit"/></value>
            <value name="B"><block type="math_single"><field name="OP">NEG</field><value name="NUM"><variables_get><field name="VAR">STOP LOSS</field></variables_get></value></block></value>
          </block>
        </value>
        <statement name="DO1">
          <block type="text_print">
            <value name="TEXT"><shadow type="text"><field name="TEXT">HOLY SHIT!</field></shadow></value>
          </block>
        </statement>
        <statement name="ELSE">
          <block type="controls_if">
            <mutation xmlns="http://www.w3.org/1999/xhtml" else="1"></mutation>
            <value name="IF0">
              <block type="contract_check_result">
                <field name="CHECK_RESULT">win</field>
              </block>
            </value>
            <statement name="DO0">
              <block type="variables_set">
                <field name="VAR">STAKE</field>
                <value><variables_get><field name="VAR">INITIAL STAKE</field></variables_get></value>
              </block>
            </statement>
            <statement name="ELSE">
              <block type="variables_set">
                <field name="VAR">STAKE</field>
                <value>
                  <block type="math_arithmetic">
                    <field name="OP">MULTIPLY</field>
                    <value name="A"><variables_get><field name="VAR">STAKE</field></variables_get></value>
                    <value name="B"><variables_get><field name="VAR">MARTIGALE</field></variables_get></value>
                  </block>
                </value>
              </block>
            </statement>
            <next>
              <block type="trade_again"/>
            </next>
          </block>
        </statement>
      </block>
    </statement>
  </block>
