<xml xmlns="https://developers.google.com/blockly/xml" is_dbot="true" collection="false">

  <variables>
    <variable id="stake">STAKE</variable>
    <variable id="init">INITIAL STAKE</variable>
    <variable id="mg">MARTINGALE</variable>
    <variable id="pred">PREDICTION</variable>

    <variable id="d1">D1</variable>
    <variable id="d2">D2</variable>
    <variable id="d3">D3</variable>
    <variable id="d4">D4</variable>
    <variable id="d5">D5</variable>
  </variables>

  <!-- TRADE DEFINITION -->
  <block type="trade_definition" deletable="false" x="0" y="0">
    <statement name="TRADE_OPTIONS">
      <block type="trade_definition_market" deletable="false" movable="false">
        <field name="MARKET_LIST">synthetic_index</field>
        <field name="SUBMARKET_LIST">random_index</field>
        <field name="SYMBOL_LIST">1HZ10V</field>
        <next>
          <block type="trade_definition_tradetype" deletable="false" movable="false">
            <field name="TRADETYPECAT_LIST">digits</field>
            <field name="TRADETYPE_LIST">overunder</field>
            <next>
              <block type="trade_definition_contracttype" deletable="false" movable="false">
                <field name="TYPE_LIST">DIGITUNDER</field>
              </block>
            </next>
          </block>
        </next>
      </block>
    </statement>

    <!-- INITIALIZATION -->
    <statement name="INITIALIZATION">
      <block type="variables_set">
        <field name="VAR" id="stake">STAKE</field>
        <value name="VALUE">
          <block type="math_number"><field name="NUM">1</field></block>
        </value>
        <next>
          <block type="variables_set">
            <field name="VAR" id="init">INITIAL STAKE</field>
            <value name="VALUE">
              <block type="math_number"><field name="NUM">1</field></block>
            </value>
            <next>
              <block type="variables_set">
                <field name="VAR" id="mg">MARTINGALE</field>
                <value name="VALUE">
                  <block type="math_number"><field name="NUM">2</field></block>
                </value>
                <next>
                  <block type="variables_set">
                    <field name="VAR" id="pred">PREDICTION</field>
                    <value name="VALUE">
                      <block type="math_number"><field name="NUM">8</field></block>
                    </value>
                  </block>
                </next>
              </block>
            </next>
          </block>
        </next>
      </block>
    </statement>

    <!-- MAIN LOOP -->
    <statement name="SUBMARKET">
      <block type="controls_repeat_ext">
        <value name="TIMES">
          <block type="math_constant"><field name="CONSTANT">INFINITY</field></block>
        </value>
        <statement name="DO">

          <!-- READ LAST 5 DIGITS -->
          <block type="variables_set">
            <field name="VAR" id="d1">D1</field>
            <value name="VALUE">
              <block type="lists_getIndex">
                <mutation at="true"></mutation>
                <field name="MODE">GET</field>
                <field name="WHERE">FROM_END</field>
                <value name="VALUE"><block type="lastDigitList"/></value>
                <value name="AT"><block type="math_number"><field name="NUM">1</field></block></value>
              </block>
            </value>
            <next>

              <block type="variables_set">
                <field name="VAR" id="d2">D2</field>
                <value name="VALUE">
                  <block type="lists_getIndex">
                    <mutation at="true"></mutation>
                    <field name="MODE">GET</field>
                    <field name="WHERE">FROM_END</field>
                    <value name="VALUE"><block type="lastDigitList"/></value>
                    <value name="AT"><block type="math_number"><field name="NUM">2</field></block></value>
                  </block>
                </value>
                <next>

                  <block type="variables_set">
                    <field name="VAR" id="d3">D3</field>
                    <value name="VALUE">
                      <block type="lists_getIndex">
                        <mutation at="true"></mutation>
                        <field name="MODE">GET</field>
                        <field name="WHERE">FROM_END</field>
                        <value name="VALUE"><block type="lastDigitList"/></value>
                        <value name="AT"><block type="math_number"><field name="NUM">3</field></block></value>
                      </block>
                    </value>
                    <next>

                      <block type="variables_set">
                        <field name="VAR" id="d4">D4</field>
                        <value name="VALUE">
                          <block type="lists_getIndex">
                            <mutation at="true"></mutation>
                            <field name="MODE">GET</field>
                            <field name="WHERE">FROM_END</field>
                            <value name="VALUE"><block type="lastDigitList"/></value>
                            <value name="AT"><block type="math_number"><field name="NUM">4</field></block></value>
                          </block>
                        </value>
                        <next>

                          <block type="variables_set">
                            <field name="VAR" id="d5">D5</field>
                            <value name="VALUE">
                              <block type="lists_getIndex">
                                <mutation at="true"></mutation>
                                <field name="MODE">GET</field>
                                <field name="WHERE">FROM_END</field>
                                <value name="VALUE"><block type="lastDigitList"/></value>
                                <value name="AT"><block type="math_number"><field name="NUM">5</field></block></value>
                              </block>
                            </value>

                            <!-- CONDITION -->
                            <next>
                              <block type="controls_if">
                                <value name="IF0">
                                  <block type="logic_operation">
                                    <field name="OP">AND</field>
                                    <value name="A">
                                      <block type="logic_compare">
                                        <field name="OP">GTE</field>
                                        <value name="A"><block type="variables_get"><field name="VAR" id="d1"/></block></value>
                                        <value name="B"><block type="math_number"><field name="NUM">7</field></block></value>
                                      </block>
                                    </value>
                                    <value name="B">
                                      <block type="logic_compare">
                                        <field name="OP">GTE</field>
                                        <value name="A"><block type="variables_get"><field name="VAR" id="d2"/></block></value>
                                        <value name="B"><block type="math_number"><field name="NUM">7</field></block></value>
                                      </block>
                                    </value>
                                  </block>
                                </value>
                                <statement name="DO0">
                                  <block type="controls_flow_statements">
                                    <field name="FLOW">BREAK</field>
                                  </block>
                                </statement>
                              </block>
                            </next>

                          </block>
                        </next>
                      </block>
                    </next>
                  </block>
                </next>
              </block>

            </next>
          </block>

        </statement>

        <!-- BUY -->
        <next>
          <block type="trade_definition_tradeoptions">
            <mutation has_prediction="true"></mutation>
            <field name="DURATIONTYPE_LIST">t</field>
            <value name="DURATION"><block type="math_number"><field name="NUM">1</field></block></value>
            <value name="AMOUNT"><block type="variables_get"><field name="VAR" id="stake"/></block></value>
            <value name="PREDICTION"><block type="variables_get"><field name="VAR" id="pred"/></block></value>
          </block>
        </next>

      </block>
    </statement>
  </block>

  <!-- BEFORE PURCHASE -->
  <block type="before_purchase" deletable="false" x="0" y="1600">
    <statement name="BEFOREPURCHASE_STACK">
      <block type="purchase">
        <field name="PURCHASE_LIST">DIGITUNDER</field>
      </block>
    </statement>
  </block>

  <!-- AFTER PURCHASE -->
  <block type="after_purchase" x="600" y="1600">
    <statement name="AFTERPURCHASE_STACK">
      <block type="controls_if">
        <value name="IF0">
          <block type="contract_check_result">
            <field name="CHECK_RESULT">win</field>
          </block>
        </value>
        <statement name="DO0">
          <block type="variables_set">
            <field name="VAR" id="stake">STAKE</field>
            <value name="VALUE"><block type="variables_get"><field name="VAR" id="init"/></block></value>
          </block>
        </statement>
        <statement name="ELSE">
          <block type="variables_set">
            <field name="VAR" id="stake">STAKE</field>
            <value name="VALUE">
              <block type="math_arithmetic">
                <field name="OP">MULTIPLY</field>
                <value name="A"><block type="variables_get"><field name="VAR" id="stake"/></block></value>
                <value name="B"><block type="variables_get"><field name="VAR" id="mg"/></block></value>
              </block>
            </value>
          </block>
        </statement>
        <next><block type="trade_again"/></next>
      </block>
    </statement>
  </block>

</xml>
